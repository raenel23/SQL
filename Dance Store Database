CREATE TABLE dance_store (id integer primary key, name TEXT, brand TEXT, gender TEXT, size TEXT, price INTEGER) ;
INSERT INTO dance_store VALUES ( 1, "Leotard","Capezio", "Female", "L", 21) ;
